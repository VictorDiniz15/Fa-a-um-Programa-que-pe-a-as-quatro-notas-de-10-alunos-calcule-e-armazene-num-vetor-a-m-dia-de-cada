cont = 2
resultado1 = []

while cont <= 4:
    nota1 = float(input('Aluno 01 digite sua primeira nota: '))
    nota2 = float(input('Aluno 01 digite sua segunda nota: '))
    nota3 = float(input('Aluno 01 digite sua terceira nota: '))
    nota4 = float(input('Aluno 01 digite sua quarta nota: '))
    media = (nota1 + nota2 + nota3 + nota4)/4
    cont += 3
    resultado1.append(media)

print('####################################')

